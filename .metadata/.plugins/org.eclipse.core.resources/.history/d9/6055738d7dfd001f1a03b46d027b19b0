package com.module1;

public class If_Else 
{
	public static void main(String[] args) 
	{
		int num1 = 10;
		int num2 = 20;
		
		// Only if block
		if(num1 > num2)
		{
			System.out.println("Number 1 is Greater");
		}
		
		// If else block
		if(num1 > num2)
		{
			System.out.println("Number 1 is Greater");
		}else
		{
			System.out.println("Number 2 is Greater");	
		}
	}
}
