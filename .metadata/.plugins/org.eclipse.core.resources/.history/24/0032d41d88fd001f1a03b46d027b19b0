package com.module1;

public class MaximumOfThreeNumber 
{
	public static void main(String[] args) 
	{
		
	}
	
	public static int maxNumber(int num1, int num2, int num3)
	{
		if(num1 > num2)
		{
			if(num2 > num3)
			{
				return num1;
			}
		}else 
		{
			if(num2 > num3)
			{
				return num2;
			}else
			{
				return num3;
			}
		}
	}
}
