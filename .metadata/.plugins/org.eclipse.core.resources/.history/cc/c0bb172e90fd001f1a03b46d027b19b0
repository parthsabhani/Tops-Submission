package com.module1;

class FirstClass
{
	public void method1()
	{
		System.out.println("Method 1");
	}
}

class SecondClass extends FirstClass
{
	public void method1()
	{
		System.out.println("Method 1");
	}
}

public class OverridingProgram 
{
	public static void main(String[] args) 
	{
		SecondClass s = new SecondClass();
		s.method1();
	}
}
