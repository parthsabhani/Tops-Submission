package com.module1;

public class MaximumOfThreeNumber 
{
	public static void main(String[] args) 
	{
		int num1=10;
		int num2=20;
		int num3=30;
		maxNumber(num1, num2, num3);
	}
	
	public static void maxNumber(int num1, int num2, int num3)
	{
		if(num1 > num2)
		{
			if(num2 > num3)
			{
				System.out.println("Number 1 is Greatter among all.");
			}
		}else 
		{
			if(num2 > num3)
			{
				System.out.println("Number 2 is Greatter among all.");
			}else
			{
				System.out.println("Number 3 is Greatter among all.");
			}
		}
	}
}
